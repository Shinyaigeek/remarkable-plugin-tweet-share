{
  "name": "remarkdown-plugin-tweet-share",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Shinyaigeek/remarkdown-plugin-tweet-share.git",
  "author": "shinyaigeek <PhilisPaxil@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint:staged": "lint-staged"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "bili": "^5.0.5",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "remarkable": "^2.0.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "remarkable": "2.x"
  },
  "lint-staged": {
    "!(*test).ts": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  }
